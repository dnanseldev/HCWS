SELECT DISTINCT ESTOQUE.FCN_INST_ONERACAO(X.COD_INSTITUICAO, X.COD_ESTRUTURA) COD_INSTITUICAO, Y.NOM_FANTASIA INSTITUICAO 
FROM USUARIO_ESTRUTURA_ORCAMENTARIA X, V_INSTITUICAO_COMPRAS Z, V_INSTITUICAO_ORDEM_SERVICO Y
WHERE NUM_USER_BANCO = 62109--1415--FC_NUM_USER_BANCO
  AND ESTOQUE.FCN_INST_ONERACAO(X.COD_INSTITUICAO, X.COD_ESTRUTURA) = Z.COD_INSTITUICAO
  AND Z.COD_INSTITUICAO = Y.COD_INSTITUICAO(+)
  AND Z.COD_INST_SIST_CC = 1;
  
  --5674
  
  SELECT TA.COD_TIPO_AQUISICAO, TA.DSC_TIPO_AQUISICAO
FROM USUARIO_TIPO_AQUISICAO UTA, TIPO_AQUISICAO TA
WHERE UTA.NUM_USER_BANCO= 62109--1415
  AND UTA.COD_TIPO_AQUISICAO=TA.COD_TIPO_AQUISICAO
  AND UTA.COD_SISTEMA = 72
ORDER BY DSC_TIPO_AQUISICAO;
  
  
  SELECT * 
   FROM usuario u
   WHERE u.nom_usuario_banco = 'SANDRAR';
   --3334 'MTIMAMURA'
   ---'RBORDONAL';
   
   
 SELECT *
  FROM usuario u
  WHERE u.num_user_banco = 62109;
    
